<Window x:Class="PCVerwaltung.PurchaseWindows.PurchHWKWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PCVerwaltung"
        mc:Ignorable="d"
        Title="PurchHWKWindow" Height="520" Width="800">
    <Grid>
        <Label Content="Hardware Komponenten" Height="50" VerticalAlignment="Top" HorizontalContentAlignment="Center" FontWeight="Bold"></Label>
        <Button x:Name="CPUButton" Content="CPU" Click="CPUClick" Height="200" Width="400" Margin="0,40,393.6,250.4"></Button>
        <Button x:Name="MainboardButton" Content="Mainboard" Click="MainboardClick" Height="200" Margin="400,240,-0.4,50.4"/>
        <Button x:Name="SSDButton" Content="SSD" Click="SSDClick" Height="200" Margin="400,40,-0.4,250.4"/>
        <Button x:Name="MonitorButton" Content="Monitor" Click="MonitorClick" Width="400" Height="200" Margin="0,240,393.6,50.4"></Button>
        <Button x:Name="BackButton" Content="Back" Click="BackClick" Height="50" Margin="0,440,-0.4,0.4"></Button>
    </Grid>
</Window>
